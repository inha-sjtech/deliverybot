#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'livox-frame-remap','console_scripts','livox_frame_remap_node'
__requires__ = 'livox-frame-remap'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('livox-frame-remap', 'console_scripts', 'livox_frame_remap_node')()
    )
